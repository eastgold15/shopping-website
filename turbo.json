{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "i": {
      "cache": false
    },
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}